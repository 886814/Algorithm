   "source": [
    "N, K = map(int,input().split())\n",
    "dp = [[0] * (K+1) for _ in range(N+1)]\n",
    "dp[0][0] = 1\n",
    "\n",
    "for i in range(N+1):\n",
    "    for j in range(1,K+1):\n",
    "        dp[i][j] = (dp[i-1][j] + dp[i][j-1]) % 1000000000\n",
    "\n",
    "print(str(dp[N][K]))"
   ]
  }
