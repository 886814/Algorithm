    "X=int(input())\n",
    "\n",
    "line = 1\n",
    "while X > line:\n",
    "    X -= line\n",
    "    line += 1\n",
    "    \n",
    "if line % 2 == 0:\n",
    "    a = X\n",
    "    b = line - X + 1\n",
    "else:\n",
    "    a = line - X + 1\n",
    "    b = X\n",
    "    \n",
    "print(a,'/',b, sep = '')"
   ]
