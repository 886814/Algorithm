    "def solution(s):\n",
    "    numbers = {'zero':0,'one':1,'two':2,'three':3,'four':4,'five':5,'six':6,'seven':7,'eight':8,'nine':9}\n",
    "\n",
    "    for i in numbers.keys():\n",
    "        if i in s:\n",
    "            s = s.replace(i,str(numbers[i]))\n",
    "    \n",
    "    return int(s)"
